+        Actions actions=new Actions(driver);
+        actions.clickAndHold(ekran).moveByOffset(-50, 30)
+                .moveByOffset(-30, -30)
+                .moveByOffset(50, -30)
+                .moveByOffset(30, 50)
+                .moveByOffset(-50, 30)
+                .moveByOffset(-30, -30)
+                .moveByOffset(50, -30)
+                .moveByOffset(30, 50)
+                .moveByOffset(-50, 30)
+                .moveByOffset(-30, -30)
+                .moveByOffset(50, -30)
+                .moveByOffset(30, 50)
+                .moveByOffset(-50, 30)
+                .moveByOffset(-30, -30)
+                .moveByOffset(50, -30)
+                .moveByOffset(30, 50)
+                .moveByOffset(-50, 30)
+                .moveByOffset(-30, -30)
+                .moveByOffset(50, -30)
+                .moveByOffset(30, 50).
+                release().perform();
+        warten(3);
+        //- Çizimden sonra clear butonuna basınız
